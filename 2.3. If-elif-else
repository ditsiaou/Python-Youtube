grade=float(input(' Insert the grade of John (selection  0-20)\n'))
#print(grade+4)

if grade>0 and grade<=5:
    print(' The student belongs in the Category_1')
elif grade>5 and grade<=10:
    print(' The student belongs in the Category_2')
elif grade>10 and grade<=15:
    print(' The student belongs in the Category_3')
elif grade>15 and grade<=20:
    print(' The student belongs in the Category_4')
else:
    print('Something is wrong')
    
grade_2=float(input(' Insert the grade of John (selection  0-20)\n'))
#print(grade+4)

if grade_2>=0 :
    print(' The student belongs in the Category_1')
elif grade_2>=10:
    print(' The student belongs in the Category_2')
